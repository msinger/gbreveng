set_io data[7]     J10

# Slave R/W
set_io n_rd        D8
set_io n_wr        A9

# Slave Chip Selects
set_io n_cs_rom    B8  # A15 (ROM CS) on cartridge connector
set_io n_cs_xram   M15 # nCS (RAM CS) on cartridge connector

# Slave Clock & Reset
set_io phi         F7
set_io n_reset     J11

# Slave Bus Level Shifters
set_io n_soe       P16
set_io sdir        P14

# VRAM Address Bus
set_io vadr[0]     E16
