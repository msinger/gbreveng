# Buttons
set_io btn[0]      M1
set_io btn[1]      K4
set_io btn[2]      L1
set_io btn[3]      L4

# FT245 parallel FIFO
set_io ft245_d[0]  B10
set_io ft245_d[1]  B12
set_io ft245_d[2]  B13
set_io ft245_d[3]  A15
set_io ft245_d[4]  A16
set_io ft245_d[5]  B14
set_io ft245_d[6]  B15
set_io ft245_d[7]  C14
set_io ft245_n_rxf D13
set_io ft245_n_txe E10
set_io ft245_n_rd  D11
set_io ft245_n_wr  C11
set_io ft245_siwu  C13

# Audio PWMs
set_io chl         B2
set_io chr         E4
set_io chm         F5

# Joypad
set_io p10         C3
set_io p11         D3
set_io p12         E5
set_io p13         D4
set_io p14         B3
set_io p15         C4

# Master Address Bus
set_io madr[0]     L10
set_io madr[1]     R10
