# For the Game Boy reverse eng. FPGA board

# Clock & Reset
set_io clk12m      J3
set_io clk16m      C8
set_io clk16m_en   F9

# LEDs
set_io led[0]      N4
set_io led[1]      R1
set_io led[2]      M5
set_io led[3]      P2
set_io led[4]      M4
set_io led[5]      P1
set_io led[6]      N3
set_io led[7]      L5
set_io led[8]      M3
set_io led[9]      M6
set_io led[10]     N2
set_io led[11]     L7
set_io led[12]     M2
set_io led[13]     K5
set_io led[14]     L3
set_io led[15]     L6

# Switches
set_io sw[0]       K3
set_io sw[1]       K1
set_io sw[2]       E2
set_io sw[3]       H5
set_io sw[4]       E3
set_io sw[5]       G4
set_io sw[6]       D1
set_io sw[7]       G5
set_io sw[8]       D2
set_io sw[9]       F4
set_io sw[10]      C2
set_io sw[11]      C1
