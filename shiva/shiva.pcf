set_io madr[3]     N9
set_io madr[4]     P7
set_io madr[5]     T8
set_io madr[6]     T7
set_io madr[7]     L9
set_io madr[8]     T6
set_io madr[9]     P8
set_io madr[10]    T5
set_io madr[11]    M8
set_io madr[12]    P6
set_io madr[13]    N7
set_io madr[14]    T3
set_io madr[15]    R5
set_io madr[16]    P10
set_io madr[17]    N10
set_io madr[18]    T10
set_io madr[19]    T11
set_io madr[20]    T15

# Master Data Bus
set_io mdata[0]    R3
set_io mdata[1]    P5
set_io mdata[2]    T2
set_io mdata[3]    N5
set_io mdata[4]    R2
set_io mdata[5]    P4
set_io mdata[6]    T1
set_io mdata[7]    N6

# Master R/W
set_io n_mrd       R6
set_io n_mwr       M9
set_io n_prog      R9

# Master Chip Selects
set_io n_mcs_rom   T14 # A15 (ROM CS) on cartridge connector
set_io n_mcs_xram  M11 # nCS (RAM CS) on cartridge connector
set_io n_mcs_wram  R16 # WRAM
set_io n_mcs_crom  N12 # onboard cartridge ROM
set_io n_mcs_cram  L11 # onboard cartridge RAM

# Master Clock & Reset
set_io mphi        K9
set_io n_mreset    R4

# Master Bus Level Shifters
set_io n_moe       P9  # output enable for n_mrd, n_mwr, n_mcs_xram, n_mcs_rom and madr[14:13]
set_io n_moed      M7  # output enable for mdata[7:0]
set_io mdir        T13 # direction for mdata[7:0]

# Slave Address Bus
set_io adr[0]      A6
set_io adr[1]      D7
set_io adr[2]      C6
set_io adr[3]      B6
set_io adr[4]      B7
